  // For HighLight

  "todohighlight.isEnable": true,
  "todohighlight.isCaseSensitive": true,
  "todohighlight.keywords": [
    "DEBUG:",
    "REVIEW:",
    "....:",
    {
      "text": ".....:",
      "color": "#f8e804",
      "backgroundColor": "#b82121",
      "overviewRulerColor": "grey",
      "isWholeLine": true
    },
    {
      "text": "...:",
      "color": "#eceaea",
      "backgroundColor": "#08179c",
      "isWholeLine": true,
      "font-style": "none"
    },
    {
      "text": "..:",
      "color": "#74f4f8",
      "border": "none",
      "backgroundColor": "#8500ad",
      "isWholeLine": true
      //other styling properties goes here ...
    },
    {
      "text": ".:",
      "color": "#eeede2",
      "border": "none",
      "backgroundColor": "#074eb9",
      "isWholeLine": true
      //other styling properties goes here ...
    },
    {
      "text": "__:",
      "color": "#cbf804",
      "backgroundColor": "#88254b",
      "overviewRulerColor": "grey",
      "isWholeLine": true
    },
    {
      "text": "NOTE:",
      "color": "#eeede2",
      "backgroundColor": "#b82121",
      "overviewRulerColor": "grey",
      "isWholeLine": false
    },
    {
      "text": ":::",
      "color": "#eeede2",
      "backgroundColor": "#984d6f",
      "overviewRulerColor": "grey",
      "isWholeLine": true
    }
  ],
  //highlight `TODO:`,`FIXME:` or content between parentheses
  "todohighlight.defaultStyle": {
    "color": "#cbf804",
    "backgroundColor": "#b60f4f",
    "overviewRulerColor": "#7c1414",
    "cursor": "pointer",
    "border": "none",
    "borderRadius": "2px",
    "isWholeLine": true
    //other styling properties goes here ...
  },
  "todohighlight.include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss"
  ],
  "todohighlight.exclude": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/dist/**",
    "**/build/**",
    "**/.vscode/**",
    "**/.github/**",
    "**/_output/**",
    "**/*.min.*",
    "**/*.map",
    "**/.next/**"
  ],
  "todohighlight.maxFilesForSearch": 5120,
  "todohighlight.toggleURI": false,

  // Better Comment
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "backgroundColor": "transparent"
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },

    // custom color
    {
      "tag": "Note",
      "color": "#8aff04",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "backgroundColor": "transparent"
    },
    {
      "tag": "-",
      "color": "#349ba8",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": ".",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "~",
      "color": "#6f8083",
      "strikethrough": false,
      "backgroundColor": "transparent"
    }
  ]
}

